<?xml version="1.0" encoding="utf-8" ?>
<CurrenciesSheet xmlns="clr-namespace:RaynMaker.Blade.DataSheetSpec;assembly=RaynMaker.Blade" 
                 xmlns:s="clr-namespace:System;assembly=mscorlib"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 MaxAgeInDays="7">
    <CurrenciesSheet.Resources>
        <Currency x:Key="Euro" Name="Euro" />
    </CurrenciesSheet.Resources>
    
    <p:StaticResource ResourceKey="Euro"/>

    <!-- http://www.finanzen.net/waehrungsrechner/ -->
    
    <Currency Name="Dollar">
        <Translation To="{p:StaticResource Euro}" Timestamp="2015-07-19" Rate="0.9234"/>
    </Currency>
</CurrenciesSheet>

